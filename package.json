    "github"
  ],
  "author": "laymonage",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/spotify-api": "^0.0.20",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "prettier": "2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
