{
  "name": "spotify-saved-tracks",
  "version": "0.0.0",
  "private": true,
  "description": "Save your Spotify saved tracks to a file",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "format-data": "prettier --write data",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laymonage/spotify-saved-tracks.git"
  },
